    <serviceTask id="taskCrearSolicitud" name="Crear Solicitud" activiti:class="org.alfresco.repo.workflow.activiti.script.AlfrescoScriptDelegate">
      <extensionElements>
        <activiti:field name="script">
          <activiti:string><![CDATA[<import resource="classpath:alfresco/extension/workflows/supportTickets/personal.js">

logger.log('Creando solicitud...' + rqwf_puesto);
execution.setVariable('rqwf_approveCount', 0);

var solicitud = crearSolicitudRequisicion(rqwf_solicitante,
              rqwf_division,
              rqwf_circuito,
              rqwf_centroCostos,
              bpm_workflowDescription,
              rqwf_numPuestos,
              rqwf_localidad,
              rqwf_nombrePuestoJefe,
              rqwf_nuevaCreacion,
              rqwf_sustitucion,
              rqwf_incapacidad,
              rqwf_isPromocion,
              rqwf_isCambio,
              rqwf_isApoyo,
              rqwf_sustituyeA,
              rqwf_fechaInicioSustitucion,
              rqwf_isTiempoIndefinido,
              rqwf_isTemporal,
              rqwf_tiempoTemporal,
              rqwf_isSupervisaPersonal,
              rqwf_numSupervisa,
              rqwf_sexoCandidato,
              rqwf_estadoCivil,
              rqwf_edadMinima,
              rqwf_edadMaxima,
              rqwf_requiereViajar,
              rqwf_isFrecuenteViajar,
              rqwf_isEventualViajar,
              rqwf_escolaridadCandidato,
	          rqwf_especialidadCandidato,
              rqwf_isExperienciaLaboral,
              rqwf_aniosExperienciaLaboral,
              rqwf_idioma,
              rqwf_numPorcentajeIdiomaHablar,
              rqwf_numPorcentajeIdiomaLeer,
              rqwf_numPorcentajeIdiomaTraducir,
              rqwf_numPorcentajeIdiomaEscrito,
              rqwf_otroIdioma,
              bpm_comment,
              rqwf_entrevistador1,
              rqwf_entrevistador2,
              rqwf_candidatoSugerido1,
              rqwf_candidatoSugerido2);

execution.setVariable( 'rqwf_solicitudFolio', solicitud);

var division = execution.getVariable('rqwf_division');
var group = 'GROUP_RECURSOS_HUMANOS_'+division;

var replacedGroup = group.replace(/\s/g, "_");
logger.debug("Asignando al grupo :" + replacedGroup);
execution.setVariable('rqwf_areaRh' , replacedGroup);]]></activiti:string>
        </activiti:field>
        <activiti:field name="runAs">
          <activiti:string><![CDATA[admin]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow1" sourceRef="startRequisicion" targetRef="taskCrearSolicitud">
      <extensionElements>
        <activiti:executionListener event="start" class="org.alfresco.repo.workflow.activiti.listener.ScriptExecutionListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[logger.debug("Asignado variables-" );
logger.debug('Puesto solicitado' +rqwf_puesto);
logger.debug('Numero de puestos solicitado' + rqwf_numPuestos);
logger.debug('Idioma ...' + rqwf_otroIdioma);]]></activiti:string>
          </activiti:field>
        </activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
