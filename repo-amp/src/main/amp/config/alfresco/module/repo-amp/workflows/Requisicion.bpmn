execution.setVariable('rqwf_comentarios', task.getVariable('rqwf_comentarios'));
var nameAutorizador =  person.properties.firstName + ' '  + person.properties.lastName;
execution.setVariable('rqwf_autorizador',  nameAutorizador);
task.setVariable('bpm_comment' ,  task.getVariable('rqwf_comentarios'));

execution.setVariable('rqwf_fechaAutAdmin', new java.util.Date());
execution.setVariable('rqwf_nombreAdmin',nameAutorizador);]]></activiti:string>
